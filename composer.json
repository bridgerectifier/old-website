{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "A modern WordPress stack",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "deliciousbrains/wp-migrate-db-pro",
        "version": "1.7.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "http://packages.losource.net/wp-migrate-db-pro-1.7.1.zip"
        },
        "require" : {
          "composer/installers": "v1.0.12"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "deliciousbrains/wp-migrate-db-pro-media-files",
        "version": "1.4.7",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "http://packages.losource.net/wp-migrate-db-pro-media-files-1.4.7.zip"
        },
        "require" : {
          "composer/installers": "v1.0.12"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.7.2",
    "oscarotero/env": "^1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "deliciousbrains/wp-migrate-db-pro": "1.7.1",
    "deliciousbrains/wp-migrate-db-pro-media-files": "1.4.7",
    "wpackagist-plugin/wordpress-seo": "4.0.2",
    "wpackagist-plugin/jetpack": "4.4.2",
    "wpackagist-plugin/akismet": "3.2",
    "wpackagist-plugin/sendgrid-email-delivery-simplified": "1.10.3"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
